{
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "node_modules",
      "node_modules/**/*.ts",
      "dist",
      "dist/**/*.ts"
    ]
  },
  "rules": {
    "interface-name": false,
    "max-line-length": [
      true,
      140
    ],
    "member-ordering": [
      true,
      {
        "alphabetize": true,
        "order": [
          "public-static-field",
          "private-static-field",
          "static-method",
          "instance-field",
          "constructor",
          "instance-method"
        ]
      }
    ],
    "no-console": [
      true,
      "warning"
    ],
    "quotemark": false,
    "variable-name": [
      true,
      "check-format",
      "allow-leading-underscore"
    ]
  },
  "rulesDirectory": []
}
